<form action="/feuille-presence/ligne/ajouter" method="post">
    <div>
        <label for="jour">Jour :</label>
        <input type="date" id="jour" name="jour" required>
    </div>
    <div>
        <label for="debut">Début :</label>
        <input type="time" id="debut" name="debut" required>
    </div>
    <div>
        <label for="fin">Fin :</label>
        <input type="time" id="fin" name="fin" required>
    </div>
    <div>
        <label for="matiere">Matière :</label>
        <input type="text" id="matiere" name="matiere" required>
    </div>
    <div>
        <label for="enseignant">Enseignant :</label>
        <input type="text" id="enseignant" name="enseignant" required>
    </div>
    <div>
        <button type="submit">Confirmer</button>
    </div>
</form>





